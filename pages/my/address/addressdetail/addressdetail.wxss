
.column{
  display: flex;
  flex-direction: row; 
  text-align: end;
  justify-content: space-between;
  margin-bottom: 10rpx;
  padding: 30rpx 20rpx;
}
.end{
   display: flex;
  flex-direction: row-reverse;
}
.address-delete{
  color:#dd2525;
}
.btn{
   background: #F3F3F3;
  padding:20rpx 30rpx;
  font-weight:bold;
  margin-top:20rpx;
  border-radius:10rpx;
}
label{
  width: 200rpx;
  text-align: left;
}
.address{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 50px;
}


/*底部操作栏*/
.ft-bottom{
	width: 100%;
  position: fixed;
	bottom: 0;
	display: block;
  border-top:1px solid #4d4d4d;
  background:#404040;
  z-index: 555;
}
.ft-bottom-box{
  display: flex;
  justify-content: center;
}
.ft-bottom-operate{
	width: 80%;
	height: 90rpx;
	display: flex;
	justify-content: center;
	flex-direction: row-reverse;
	align-items: center;
  background:#DAB866;
  border:1px solid #4d4d4d;
  border-radius: 10px;
  margin:20rpx;
  color:#fff;
  font-size: 1rem;
}


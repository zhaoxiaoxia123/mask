
.column{
  display: flex;
  flex-direction: row; 
  text-align: end;
  justify-content: space-between;
  margin-bottom: 10rpx;
  padding: 40rpx 20rpx;

  
}
.end{
   display: flex;
  flex-direction: row-reverse;
}

.btn{
   background: #F3F3F3;
  padding:20rpx 30rpx;
  font-weight:bold;
  margin-top:20rpx;
  border-radius:10rpx;
}

/*底部操作栏*/
.bottom{
	width: 94%;
  position: fixed;
	bottom: 0;
	display: block;
  margin-bottom:20rpx;
  border-radius: 10px;
  border:1px solid #4d4d4d;
}
.bottom-operate{
	width: 100%;
	height: 90rpx;
	display: flex;
	justify-content: center;
	flex-direction: row-reverse;
	align-items: center;
}
label{
  width: 200rpx;
  text-align: left;
}
.address{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 50px;
}
